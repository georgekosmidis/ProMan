
@{
    string start = PoseidonWeb.Helpers.Classic.Sessions.Get<DateTime>( "StartDate" ).ToString( "d MMM yyy" );
    string end =  PoseidonWeb.Helpers.Classic.Sessions.Get<DateTime>( "EndDate" ).ToString( "d MMM yyy" );
    string title = (ViewData["Title"]).ToString();
    string icon = (ViewData["icon"]).ToString();
    string period = start + " - " + end;
}

<section class="content-header">
    <h1 class="pull-left">
        <span class="pull-left fa-stack">
            <i class="fa fa-circle fa-stack-2x"></i>
            <i class="fa fa-@icon fa-stack-1x fa-inverse"></i>
        </span> 
        @title
        <small>Control panel</small>
    </h1>
    <button class="btn btn-primary btn-flat pull-right" id="daterange-btn">
        <i class="fa fa-calendar"></i> <span>@period</span>
        <i class="fa fa-caret-down"></i>
    </button>
    @*<ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Dashboard</li>
    </ol>*@
  
</section>
